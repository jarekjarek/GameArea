<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is not required if you don't need any extra configuration. -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

  <!-- Write your navigation rules here. You should use CDI for creating 
    @Named managed beans! -->
  
  <application>
    <locale-config>
      <default-locale>pl</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>
    
    
  </application>
	<managed-bean>
	<managed-bean-name>userBean</managed-bean-name>
	<managed-bean-class>gryczko.jaroslaw.beans.UserBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	
	</managed-bean>
	<managed-bean>
	<managed-bean-name>noticesBean</managed-bean-name>
	<managed-bean-class>gryczko.jaroslaw.beans.NoticesBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	
	</managed-bean>
	
	<managed-bean>
	<managed-bean-name>showBean</managed-bean-name>
	<managed-bean-class>gryczko.jaroslaw.beans.ShowBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	
	</managed-bean>
	
	<managed-bean>
	<managed-bean-name>showUserBean</managed-bean-name>
	<managed-bean-class>gryczko.jaroslaw.beans.ShowUserBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	
	</managed-bean>
  
  
</faces-config>
